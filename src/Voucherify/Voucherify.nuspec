      </group>
      <group targetFramework="net45">
        <dependency id="Newtonsoft.Json" version="9.0.1" />
        <dependency id="System.Net.Http" version="4.3.4" />
      </group>
      <group targetFramework="net20">
        <dependency id="Newtonsoft.Json" version="9.0.1" />
      </group>
      <group targetFramework="netstandard20">
        <dependency id="Newtonsoft.Json" version="9.0.1" />
        <dependency id="System.Net.Http" version="4.3.4" />
      </group>
    </dependencies>
  </metadata>
  <files>
